{
  "title": "Vlan Post",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "vlans"
  ],
  "properties": {
    "vlans": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "null"
          },
          "name": {
            "type": "string"
          },
          "num_vlan": {
            "type": ["integer","null"]
          },
          "environment": {
            "type": "integer"
          },
          "description": {
            "type": ["string","null"]
          },
          "acl_file_name": {
            "type": ["string","null"]
          },
          "acl_valida": {
            "type": "boolean"
          },
          "acl_file_name_v6": {
            "type": ["string","null"]
          },
          "acl_valida_v6": {
            "type": "boolean"
          },
          "active": {
            "type": "boolean"
          },
          "vrf": {
            "type": ["string","null"]
          },
          "acl_draft": {
            "type": ["string","null"]
          },
          "acl_draft_v6": {
            "type": ["string","null"]
          },
          "create_networkv4": {
            "type": ["object","null"],
            "properties": {
              "network_type": {
                "type": ["integer","null"]
              },
              "environmentvip": {
                "type": ["integer","null"]
              },
              "prefix": {
                "type": ["integer","null"]
              }
            }
          },
          "create_networkv6": {
            "type": ["object","null"],
            "properties": {
              "network_type": {
                "type": ["integer","null"]
              },
              "environmentvip": {
                "type": ["integer","null"]
              },
              "prefix": {
                "type": ["integer","null"]
              }
            }
          }
        },
        "required": [
          "name",
          "environment"
        ]
      }
    }
  }
}

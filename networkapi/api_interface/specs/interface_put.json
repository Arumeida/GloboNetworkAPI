{
  "title": "Interface Post",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "interfaces"
  ],
  "properties": {
    "interfaces": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "interface": {
            "type": "string"
          },
          "description": {
            "type": [
              "string",
              "null"
            ]
          },
          "native_vlan": {
            "type": "string"
          },
          "type": {
            "type": "integer"
          },
          "protected": {
            "type": "boolean"
          },
          "equipment": {
            "type": "integer"
          },
          "front_interface": {
            "type": [
              "integer",
              "null"
            ]
          },
          "back_interface": {
            "type": [
              "integer",
              "null"
            ]
          }
        },

        "required": [
          "id",
          "interface",
          "type",
          "protected",
          "equipment",
          "front_interface",
          "back_interface"
        ]
      }
    }
  }
}

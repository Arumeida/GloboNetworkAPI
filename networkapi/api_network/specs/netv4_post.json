{
  "title": "NetworkIPv4 Post",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "networks"
  ],
  "properties": {
    "networks": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "oct1": {
            "type": "integer"
          },
          "oct2": {
            "type": "integer"
          },
          "oct3": {
            "type": "integer"
          },
          "oct4": {
            "type": "integer"
          },
          "block": {
            "type": "integer"
          },
          "mask_oct1": {
            "type": "integer"
          },
          "mask_oct2": {
            "type": "integer"
          },
          "mask_oct3": {
            "type": "integer"
          },
          "mask_oct4": {
            "type": "integer"
          },
          "vlan": {
            "type": "integer"
          },
          "network_type": {
            "type": "integer"
          },
          "environmentvip": {
            "type": [
              "integer",
              "null"
            ]
          },
          "active": {
            "type": "boolean"
          },
          "cluster_unit": {
            "type": "string"
          }
        },
        "required": [
            "vlan",
            "network_type",
            "environmentvip"
        ]
      }
    }
  }
}
